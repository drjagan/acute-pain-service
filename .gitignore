# Acute Pain Service - Git Ignore File
# Version 1.0.0

# Configuration Files (sensitive)
config/config.php
config/.installed

# Environment Files
.env
.env.local
.env.production

# Allow production environment template for aps.sbvu.ac.in branch
!.env.production.sbvu

# Logs
logs/*.log
logs/*.txt
*.log

# Uploads (user files)
public/uploads/*
!public/uploads/.gitkeep

# Exports (generated files)
public/exports/*
!public/exports/.gitkeep

# Cache
cache/*
!cache/.gitkeep

# Temporary Files
tmp/*
temp/*
*.tmp

# IDE / Editor Files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace
.phpstorm.meta.php
_ide_helper.php

# OS Files
.DS_Store
Thumbs.db
**/.DS_Store

# Archive folder
_archive/
desktop.ini

# Composer
/vendor/
composer.lock

# NPM / Node
node_modules/
npm-debug.log
yarn-error.log
package-lock.json

# Backup Files
*.bak
*.backup
*.old
*.swp
*~

# Database Dumps
*.sql.gz
*.sql.zip
database_backup_*.sql

# Session Files
sessions/*

# Installation Wizard (delete after install)
# Uncomment after installation:
# install/

# Test Files (optional)
phpunit.xml
tests/_output/*

# Documentation Build
docs/_build/

# Sensitive Data
*.pem
*.key
*.crt
secrets.json
credentials.json

# Version Control
.git/
.gitattributes

# PHP CS Fixer
.php_cs
.php_cs.cache
.php-cs-fixer.cache

# PHPStan
phpstan.neon
.phpstan.cache

# Coverage Reports
coverage/
.coverage

# Keep empty directories
!.gitkeep
